          "(([\i-[:]][\c-[:]]*)?:)(/[^\s/][^\s]*|[^\s/][^\s]*|[^\s]?)"
        minLength = "1"
      }
      | xsd:string { pattern = "[\i-[:]][\c-[:]]+:.+" }
    }?
    & attribute ns1:schemaLocation { text }?
    & attribute xml:base { xsd:anyURI }?
    & attribute xml:id { xsd:NCName }?
    & (RuleML
       | Assert
       | Retract
       | Query
       | Atom
       | Rel
       | And
       | Or
       | Rulebase
       | Entails
       | Data
       | Skolem
       | Reify
       | Ind
       | formula
       | formula_2
       | op
       | formula_3
       | formula_4
       | formula_5
       | then
       | if
       | degree
       | meta
       | oid
       | slot
       | arg)*
  }
