<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:ruleml="http://www.ruleml.org/0.91/xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://relaxng.org/ns/structure/1.0">
  <dc:title>RuleML Custom-Built Schema</dc:title>
  <dc:version>0.91.01</dc:version>
  <dc:creator>Tara Athan (taraathan AT gmail.com)</dc:creator>
  <dc:subject>RuleML, custom-built</dc:subject>
  <dc:description>custom-built main module for a RuleML sublanguage.</dc:description>
  <dc:date>2011-05-16T13:44:33-0300</dc:date>
  <dc:language>en</dc:language>
  <dc:rights>TBD</dc:rights>
  <dc:relation>http://wiki.ruleml.org/index.php/Relax_NG</dc:relation>
  <!--
    Call parameters
    Base URL = http://www.ruleml.org/0.91/relaxng/schema_rnc.php 
    GET parameter: backbone=x1
    GET parameter: default=x3
    GET parameter: termseq=x2
    GET parameter: lng=x1
    GET parameter: propo=xf
    GET parameter: implies=x6
    GET parameter: terms=xf0f
    GET parameter: quant=x1
    GET parameter: expr=x1
    GET parameter: serial=xf
    Complete URL = 
        http://www.ruleml.org/0.91/relaxng/schema_rnc.php?backbone=x1&default=x3&termseq=x2&lng=x1&propo=xf&implies=x6&terms=xf0f&quant=x1&expr=x1&serial=xf&
  -->
  <start>
    <choice>
      <ref name="Node.choice"/>
      <ref name="edge.choice"/>
    </choice>
  </start>
  <!--
    
    ROOT NODE AND PERFORMATIVES INCLUDED
    
  -->
  <include href="performative_extension_module.rng" ns="http://www.ruleml.org/0.91/xsd">
    <start combine="choice">
      <notAllowed/>
    </start>
  </include>
  <!--
    
    ATOMIC FORMULAS INCLUDED
    
