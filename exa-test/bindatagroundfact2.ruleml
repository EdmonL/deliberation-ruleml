<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.ruleml.org/1.0/relaxng/bindatagroundfact_relaxed.rnc" type="application/relax-ng-compact-syntax"?>
<ruleml:RuleML xmlns:ruleml="http://www.ruleml.org/1.0/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ruleml:Query>
    <!-- a trivial example rulebase for testing the 'UR'-Centered Binary Ground-Datalog-Fact RuleML sublanguage -->
    <ruleml:And/>
    <ruleml:Or/>
    <ruleml:Atom>
      <ruleml:op>
        <ruleml:Rel iri="some_URI">some relation</ruleml:Rel>
      </ruleml:op>
      <ruleml:slot card="2" weight="0.5">
        <ruleml:Ind>role_name1</ruleml:Ind>
        <ruleml:Ind>role 1</ruleml:Ind>
      </ruleml:slot>
      <ruleml:Ind>constant a</ruleml:Ind>
      <ruleml:Ind iri="another_URI">constant b</ruleml:Ind>
      <ruleml:slot weight="0.4">
        <ruleml:Ind>role_name2</ruleml:Ind>
        <ruleml:Ind>role 2</ruleml:Ind>
      </ruleml:slot>
    </ruleml:Atom>
    <ruleml:formula>
      <ruleml:Atom>
        <ruleml:op>
          <ruleml:Rel iri="some_URI">another relation</ruleml:Rel>
        </ruleml:op>
        <ruleml:Ind>constant c</ruleml:Ind>
        <ruleml:Ind>constant d</ruleml:Ind>
      </ruleml:Atom>
    </ruleml:formula>
  </ruleml:Query>

  <ruleml:Assert>
    <ruleml:oid>
      <ruleml:Ind>rule (fact) label</ruleml:Ind>
    </ruleml:oid>

    <ruleml:Atom>
      <ruleml:op>
        <ruleml:Rel>yet another relation</ruleml:Rel>
      </ruleml:op>
      <ruleml:Ind iri="URI">constant e</ruleml:Ind>
      <ruleml:Ind>constant b</ruleml:Ind>
      <ruleml:slot>
        <ruleml:Ind>role_name3</ruleml:Ind>
        <ruleml:Ind>role 3</ruleml:Ind>
      </ruleml:slot>
    </ruleml:Atom>
  </ruleml:Assert>

</ruleml:RuleML>
