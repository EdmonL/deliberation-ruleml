  <!-- a syntactic example rulebase for testing the RuleML Negative Constraints sublanguage -->

  <Assert>

    <!-- No one is their own mother. -->
    <Forall>
      <Var>x</Var>
      <Implies>
        <if>
          <Atom>
            <Rel>mother</Rel>
            <Var>x</Var>
            <Var>x</Var>
          </Atom>
        </if>
        <then>
          <Or/>
        </then>
      </Implies>
    </Forall>
  </Assert>

</RuleML>
