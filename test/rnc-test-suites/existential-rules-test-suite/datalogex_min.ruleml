<?xml-model href="../../../relaxng/test/myng-b07-d7-a7-l1-p001-i40-t200-q0-e0-sf.rnc"?>
<RuleML xmlns="http://ruleml.org/spec">


  <!-- a syntactic example rulebase for testing the RuleML Existential Rules sublanguage -->

  <Assert>
    <!-- Every manager directs at least one department. -->
    <Forall>
      <Var>M</Var>
      <Implies>
        <if>
            <Atom>
              <Rel>manager</Rel>
              <Ind>x</Ind>
            </Atom>
        </if>
        <then>
          <Exists>
            <Var>P</Var>
            <Atom>
              <Rel>directs</Rel>
              <Var>M</Var>
              <Var>P</Var>
            </Atom>
          </Exists>
        </then>
      </Implies>
    </Forall>


  </Assert>

</RuleML>
